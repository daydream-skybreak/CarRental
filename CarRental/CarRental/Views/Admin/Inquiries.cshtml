@model IEnumerable<CarRental.Models.InquiresModel>
@{
    ViewData["Title"] = "Inquiries";
}
<h1>User Inquiries</h1>
<table class="table">
    <thead>
        <tr>
            <th>Id</th>
            <th>User Name</th>
            <th>Email</th>
            <th>Message</th>
            <th>Car Id</th>
            <th>User Id</th>
        </tr>
    </thead>
    <tbody>
    @if (Model != null)
    {
        foreach (var inquiry in Model)
        {
            <tr>
                <td>@inquiry.Id</td>
                <td>@inquiry.User?.Name</td>
                <td>@inquiry.User?.Email</td>
                <td>@inquiry.Message</td>
                <td>@inquiry.CarId</td>
                <td>@inquiry.UserId</td>
            </tr>
        }
    }
    </tbody>
</table>
<a href="/Admin/Index" class="btn btn-secondary">Back to Admin Dashboard</a>
